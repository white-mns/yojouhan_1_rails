= form_for(@total_megane) do |f|
  - if @total_megane.errors.any?
    #error_explanation
      %h2
        = pluralize(@total_megane.errors.count, "error")
        prohibited this total_megane from being saved:
      %ul
        - @total_megane.errors.full_messages.each do |msg|
          %li= msg
  .field
    = f.label :result_no
    %br/
    = f.number_field :result_no
  .field
    = f.label :generate_no
    %br/
    = f.number_field :generate_no
  .field
    = f.label :e_no
    %br/
    = f.number_field :e_no
  .field
    = f.label :megane_type_id
    %br/
    = f.number_field :megane_type_id
  .field
    = f.label :megane_count
    %br/
    = f.number_field :megane_count
  .actions
    = f.submit
