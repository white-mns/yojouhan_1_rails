= form_for(@payoff) do |f|
  - if @payoff.errors.any?
    #error_explanation
      %h2
        = pluralize(@payoff.errors.count, "error")
        prohibited this payoff from being saved:
      %ul
        - @payoff.errors.full_messages.each do |msg|
          %li= msg
  .field
