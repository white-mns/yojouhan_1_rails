    = f.label :payoff
    %br/
    = f.number_field :payoff
  .field
    = f.label :attack
    %br/
    = f.number_field :attack
  .field
    = f.label :support
    %br/
    = f.number_field :support
  .field
    = f.label :defense
    %br/
    = f.number_field :defense
  .field
    = f.label :destroy
    %br/
    = f.number_field :destroy
  .field
    = f.label :selling
    %br/
    = f.number_field :selling
  .field
    = f.label :income
    %br/
    = f.number_field :income
  .field
    = f.label :spending
    %br/
    = f.number_field :spending
