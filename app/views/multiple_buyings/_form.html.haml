= form_for(@multiple_buying) do |f|
  - if @multiple_buying.errors.any?
    #error_explanation
      %h2
        = pluralize(@multiple_buying.errors.count, "error")
        prohibited this multiple_buying from being saved:
      %ul
        - @multiple_buying.errors.full_messages.each do |msg|
          %li= msg
  .field
    = f.label :result_no
    %br/
    = f.number_field :result_no
  .field
    = f.label :generate_no
    %br/
    = f.number_field :generate_no
  .field
    = f.label :e_no
    %br/
    = f.number_field :e_no
  .field
    = f.label :battle_no
    %br/
    = f.number_field :battle_no
  .field
    = f.label :multiple_buying
    %br/
    = f.number_field :multiple_buying
  .actions
    = f.submit
