= form_for(@castle_structure_major_type_num) do |f|
  - if @castle_structure_major_type_num.errors.any?
    #error_explanation
      %h2
        = pluralize(@castle_structure_major_type_num.errors.count, "error")
        prohibited this castle_structure_major_type_num from being saved:
      %ul
        - @castle_structure_major_type_num.errors.full_messages.each do |msg|
          %li= msg
  .field
    = f.label :result_no
    %br/
    = f.number_field :result_no
  .field
    = f.label :generate_no
    %br/
    = f.number_field :generate_no
  .field
    = f.label :e_no
    %br/
    = f.number_field :e_no
  .field
    = f.label :build_num
    %br/
    = f.number_field :build_num
  .field
    = f.label :guard_num
    %br/
    = f.number_field :guard_num
  .field
    = f.label :goods_num
    %br/
    = f.number_field :goods_num
  .actions
    = f.submit
